{
  "name": "host-v15",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/host-v15/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/host-v15",
        "index": "apps/host-v15/src/index.html",
        "main": "apps/host-v15/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/host-v15/tsconfig.app.json",
        "assets": [],
        "styles": ["apps/host-v15/src/styles.css"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            { "type": "initial", "maximumWarning": "500kb", "maximumError": "1mb" }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": { "browserTarget": "host-v15:build:production" },
        "development": { "browserTarget": "host-v15:build:development" }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4200
      }
    }
  },
  "tags": []
}
